			<groupId>com.egoveris</groupId>
			<artifactId>te-model-common</artifactId>
		</dependency>
		<dependency>
			<groupId>com.egoveris</groupId>
			<artifactId>te-base-web</artifactId>
		</dependency>
		<dependency>
			<groupId>com.egoveris</groupId>
			<artifactId>vuc-front-ws-common</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
		 	<groupId>com.egoveris</groupId>
