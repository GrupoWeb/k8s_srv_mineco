<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
"http://www.springframework.org/dtd/spring-beans.dtd">

	<!-- ConfiguraciÃ³n del componente WebDAV para pruebas -->
<beans>
	<bean name="hostname" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="${repository.hostname}" />
	</bean>
	<bean name="port" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="${repository.port}" />
	</bean>
	<bean name="documentRoot" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="${repository.documentRoot}" />
	</bean>
	<bean name="defaultUsername" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="${repository.defaultUsername}" />
	</bean>
	<bean name="defaultPassword" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="${repository.defaultPassword}" />
	</bean>
	<bean name="connectionTimeout" class="java.lang.Integer">
		<constructor-arg type="java.lang.String"
			value="${repository.connectionTimeout}" />
	</bean>
</beans>
