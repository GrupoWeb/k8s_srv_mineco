<?xml version="1.0" encoding="UTF-8"?>
<complexComponent type="TRADE" name="Participantes"
	description="Participantes Information" zul="participantes.zul"
	composer="com.everis.tramelec.dynform.comp.ext.ParticipantesComposer"
	mappingDTO="com.egoveris.ccomplejos.base.model.ParticipantesDTO">

	<field name="participanteId" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.LongboxExt" />

	<field name="participanteType" width="90%" height="20px"
		factory="comboBoxFactory" />

	<field name="dispatcherCode" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt"/>

	<field name="personaTypeEnum" width="90%" height="20px"
		factory="comboBoxFactory" />

	<field name="docPersonaType" width="90%" height="20px"
		factory="comboBoxFactory" />

	<field name="docPersonaNum" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt"/>

	<field name="participanteNombre" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt"/>

	<field name="participanteApellido" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt"/>

	<field name="participantePorcentaje" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.DoubleboxExt"/>

	<field name="participanteAddress" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.SeparatorComplex" xmlFile="address.xml" />

	<field name="participanteEmail" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />

	<field name="participanteNumeroTelefonoFijo" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />

	<field name="participanteNumeroTelefonoMovil" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />

	<field name="contacto" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.SeparatorComplex" xmlFile="participantesecundario.xml" />


</complexComponent>