<?xml version="1.0" encoding="UTF-8"?>
<complexComponent type="TRADE" name="instalacion"
	description="Instalacion Information" zul="instalacion.zul"
	composer="com.everis.tramelec.dynform.comp.ext.InstalacionComposer"
	mappingDTO="com.egoveris.ccomplejos.base.model.InstalacionDTO">

	<field name="oigPropietaria" width="90%" height="20px"
		factory="comboBoxFactory" />
	<field name="nombreInstalacion" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />
	<field name="direccionInstalacion"
		class="com.egoveris.ffdd.render.zk.comp.ext.SeparatorComplex" xmlFile="address.xml"/>
	<field name="numeroResolucionAut" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />
	<field name="fechaEmisionResolucion" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.DateboxExt" />
	<field name="entidadEmisora" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />
	<field name="directorTecnico"
		class="com.egoveris.ffdd.render.zk.comp.ext.SeparatorComplex" xmlFile="participantes.xml" />
	<field name="tipoInstalacionDestino" width="90%" height="20px"
		factory="comboBoxFactory" />
	<field name="codigoInstalacionDestino" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />
	<field name="telefonoInstalacionDestino" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />
	<field name="contactoInstalacionDestino" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />
	<field name="razonSocial" width="90%" height="20px"
		class="com.egoveris.ffdd.render.zk.comp.ext.TextboxExt" />

</complexComponent>